{
  "cache": true,
  "cache-location": "./var/.eslintcache",
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "no-unused-expressions": ["error", { "allowTernary": true }]
  }
}